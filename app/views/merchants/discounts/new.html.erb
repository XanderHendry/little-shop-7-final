<h3>New Discount</h3>
<%= form_with url: "/merchants/#{@merchant.id}/discounts", method: :post, data: { turbo: false} do |f| %>
  <%= f.label :percentage %>
  <%= f.number_field :percentage, in: 1..100 %>
  <%= f.label :threshold %>
  <%= f.number_field :threshold %>
  <%= f.submit "Create" %>
<% end %>